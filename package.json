{
	"name": "auto-language",
	"displayName": "Auto Untitled Language",
	"description": "Automatically changes the language of untitled plaintext files using ML!",
	"publisher": "TylerLeonhardt",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:plaintext"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"// Need to do this because electron node's version isn't playing nice with the folder name":"",
		"postinstall": "node-mv ./node_modules/@tensorflow/tfjs-node/lib/napi-v7 ./node_modules/@tensorflow/tfjs-node/lib/napi-v6"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"node-mv": "~0.1.3"
	},
	"dependencies": {
		"@tensorflow/tfjs-node": "~3.1.0"
	}
}
